{
  "name": "apolloserver",
  "version": "2.0.0",
  "description": "Lambda function generated by Amplify",
  "main": "index.js",
  "license": "Apache-2.0",
  "scripts": {
    "generate": "yarn generate:prisma && yarn generate:nexus",
    "generate:prisma": "npx prisma generate",
    "generate:nexus": "yarn ts-node --transpile-only api/schema",
    "migrate": "yarn db:save && yarn db:migrate",
    "db:save": "yarn prisma migrate dev --create-only --preview-feature",
    "db:migrate": "yarn prisma migrate dev --preview-feature"
  },
  "dependencies": {
    "@prisma/client": "^2.13.1",
    "apollo-server-lambda": "^2.19.0",
    "argon2": "^0.27.1",
    "graphql": "^15.4.0",
    "jsonwebtoken": "^8.5.1",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.27.0"
  },
  "devDependencies": {
    "@prisma/cli": "^2.13.1",
    "@types/jsonwebtoken": "^8.5.0",
    "ts-node": "^9.1.1"
  }
}
